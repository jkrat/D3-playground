{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [],
  "rules": {
    "comma-dangle": 1,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "arrow-parens": [1, "as-needed"],
    "react/no-danger": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0
  },
  "parserOptions": {
    "sourceType": "module"
  }
}